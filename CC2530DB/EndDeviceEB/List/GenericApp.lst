    443          /*********************************************************************
    444           * @fn      GenericApp_ProcessZDOMsgs()
    445           *
    446           * @brief   Process response messages
    447           *
    448           * @param   none
    449           *
    450           * @return  none
    451           */
    452          static void GenericApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    453          {
    454            switch ( inMsg->clusterID )
    455            {
    456              case End_Device_Bind_rsp:
    457                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    458                {
    459                  // Light LED
    460                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    461                }
    462          #if defined( BLINK_LEDS )
    463                else
    464                {
    465                  // Flash LED to show failure
    466                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    467                }
    468          #endif
    469                break;
    470          
    471              case Match_Desc_rsp:
    472                {
    473                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    474                  if ( pRsp )
    475                  {
    476                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    477                    {
    478                      GenericApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    479                      GenericApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    480                      // Take the first endpoint, Can be changed to search through endpoints
    481                      GenericApp_DstAddr.endPoint = pRsp->epList[0];
    482          
    483                      // Light LED
    484                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    485                    }
    486                    osal_mem_free( pRsp );
    487                  }
    488                }
    489                break;
    490            }
    491          }
