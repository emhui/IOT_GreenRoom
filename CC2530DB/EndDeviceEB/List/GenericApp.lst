    511            // Shift is used to make each button/switch dual purpose.
    512            if ( shift )
    513            {
    514              if ( keys & HAL_KEY_SW_1 )
    515              {
    516              }
    517              if ( keys & HAL_KEY_SW_2 )
    518              {
    519              }
    520              if ( keys & HAL_KEY_SW_3 )
    521              {
    522              }
    523              if ( keys & HAL_KEY_SW_4 )
    524              {
    525              }
    526            }
    527            else
    528            {
    529              if ( keys & HAL_KEY_SW_1 )
    530              {
    531                // Since SW1 isn't used for anything else in this application...
    532          #if defined( SWITCH1_BIND )
    533                // we can use SW1 to simulate SW2 for devices that only have one switch,
    534                keys |= HAL_KEY_SW_2;
    535          #elif defined( SWITCH1_MATCH )
    536                // or use SW1 to simulate SW4 for devices that only have one switch
    537                keys |= HAL_KEY_SW_4;
    538          #endif
    539              }
    540          
    541              if ( keys & HAL_KEY_SW_2 )
    542              {
    543                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
