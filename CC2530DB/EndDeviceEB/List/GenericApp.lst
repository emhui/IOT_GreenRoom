    557              {
    558              }
    559          
    560              if ( keys & HAL_KEY_SW_4 )
    561              {
    562                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    563                // Initiate a Match Description Request (Service Discovery)
    564                dstAddr.addrMode = AddrBroadcast;
