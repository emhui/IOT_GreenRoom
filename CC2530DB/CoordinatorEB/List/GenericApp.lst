    408          /*********************************************************************
    409           * @fn      GenericApp_ProcessZDOMsgs()
    410           *
    411           * @brief   Process response messages
    412           *
    413           * @param   none
    414           *
    415           * @return  none
    416           */
    417          static void GenericApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    418          {
    419            switch ( inMsg->clusterID )
    420            {
    421              case End_Device_Bind_rsp:
    422                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    423                {
    424                  // Light LED
    425                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    426                }
    427          #if defined( BLINK_LEDS )
    428                else
    429                {
    430                  // Flash LED to show failure
    431                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    432                }
    433          #endif
    434                break;
    435          
    436              case Match_Desc_rsp:
    437                {
    438                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    439                  if ( pRsp )
    440                  {
    441                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    442                    {
    443                      GenericApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    444                      GenericApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    445                      // Take the first endpoint, Can be changed to search through endpoints
    446                      GenericApp_DstAddr.endPoint = pRsp->epList[0];
    447          
    448                      // Light LED
    449                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    450                    }
    451                    osal_mem_free( pRsp );
    452                  }
    453                }
    454                break;
    455            }
    456          }
