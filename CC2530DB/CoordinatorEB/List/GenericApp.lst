    642            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    643            {
    644              uint8 cmndId = inMsg.cmnd;
    645              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    646          
    647              switch ( cmndId )
    648              {
    649                case CMD_INCR:
    650                  counter += 1;  /* Increment the incoming counter */
    651                                 /* Intentionally fall through next case */
    652          
    653                case CMD_ECHO:
    654                {
    655                  userQueue_t outMsg;
    656          
    657                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    658                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    659                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    660                  break;
    661                }
    662                
    663                default:
    664                  break;  /* Ignore unknown command */    
    665              }
    666            }
    667          }
    668          #endif
