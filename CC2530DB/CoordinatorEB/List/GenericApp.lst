    510                // Initiate an End Device Bind Request for the mandatory endpoint
    511                dstAddr.addrMode = Addr16Bit;
    512                dstAddr.addr.shortAddr = 0x0000; // Coordinator
    513                ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
    514                                      GenericApp_epDesc.endPoint,
    515                                      GENERICAPP_PROFID,
    516                                      GENERICAPP_MAX_CLUSTERS, (cId_t *)GenericApp_ClusterList,
    517                                      GENERICAPP_MAX_CLUSTERS, (cId_t *)GenericApp_ClusterList,
    518                                      FALSE );
    519              }
    520          
    521              if ( keys & HAL_KEY_SW_3 )
    522              {
    523              }
    524          
    525              if ( keys & HAL_KEY_SW_4 )
    526              {
    527                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    528                // Initiate a Match Description Request (Service Discovery)
    529                dstAddr.addrMode = AddrBroadcast;
    530                dstAddr.addr.shortAddr = NWK_BROADCAST_SHORTADDR;
