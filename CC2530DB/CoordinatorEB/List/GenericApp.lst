    471           */
    472          static void GenericApp_HandleKeys( uint8 shift, uint8 keys )
    473          {
    474            zAddrType_t dstAddr;
    475          
    476            // Shift is used to make each button/switch dual purpose.
    477            if ( shift )
    478            {
    479              if ( keys & HAL_KEY_SW_1 )
    480              {
    481              }
    482              if ( keys & HAL_KEY_SW_2 )
    483              {
    484              }
    485              if ( keys & HAL_KEY_SW_3 )
    486              {
    487              }
    488              if ( keys & HAL_KEY_SW_4 )
    489              {
    490              }
    491            }
    492            else
    493            {
    494              if ( keys & HAL_KEY_SW_1 )
    495              {
    496                // Since SW1 isn't used for anything else in this application...
    497          #if defined( SWITCH1_BIND )
    498                // we can use SW1 to simulate SW2 for devices that only have one switch,
    499                keys |= HAL_KEY_SW_2;
    500          #elif defined( SWITCH1_MATCH )
    501                // or use SW1 to simulate SW4 for devices that only have one switch
    502                keys |= HAL_KEY_SW_4;
    503          #endif
    504              }
    505          
    506              if ( keys & HAL_KEY_SW_2 )
    507              {
    508                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
